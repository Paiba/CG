

class Circulo{
public:
    float pos_x;
    float pos_y;
    float radius;
    int id;
    string cor;

    Circulo();
    void setAtt(float x, float y, int iD, float raio, string fill);
};

Circulo::Circulo(){}

void Circulo::setAtt(float x, float y, int iD, float raio, string fill){
    pos_x = x;
    pos_y = y;
    radius = raio;
    id = iD; 
    cor = fill;
}
